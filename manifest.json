{
    "manifest_version": 3,
    "name": "YT Quicker To Junk",
    "short_name": "YT Quicker",
    "description": "Quick Junk Buttons",
    "version": "0.1",
    "author": "KellyCode",
    "icons": {
        "16": "rosetta16.png",
        "32": "rosetta32.png",
        "48": "rosetta48.png",
        "128": "rosetta128.png",
        "256": "rosetta256.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "run_at": "document_idle",
            "css": ["content.css"],
            "js": ["jquery-3.5.1.min.js", "content.js"]
        }
    ],
    "permissions": [
        "webNavigation", "scripting"
    ]
}